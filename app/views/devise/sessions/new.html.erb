<div class="container well formborder">
        <div class="col-md-9 center-block no-float">
            <h2><span class="glyphicon glyphicon-user"></span>Iniciar sesión</h2>
              <%= form_for(resource, as: resource_name, url: session_path(resource_name),html: {role: "form"}) do |f| %>
            <div class="form-group login">
                <%= f.label :email,'Email' %><br />
                <%= f.email_field :email, autofocus: true, class: "form-control" %>
            </div>

            <div class="form-group login">
                <%= f.label :password ,'Contraseña' %><br />
                <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
            </div>

            <% if devise_mapping.rememberable? -%>
              <div class="form-group">
                  <%= f.check_box :remember_me %>
                  <%= f.label :remember_me, 'Recordarme' %>
              </div>
            <% end -%>
                <div class="actions">
                    <%= f.submit "Entrar", class: "btn btn-success" %>
                </div>
            <% end %>

            <%= render "devise/shared/links" %>
        </div>
    </div>